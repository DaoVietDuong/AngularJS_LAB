    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-latest": "^6.24.1",
    "babel-register": "^6.26.0",
    "chalk": "^2.3.0",
    "html-loader": "^0.5.1",
    "localtunnel": "^1.8.3",
    "compression": "^1.7.1",
    "css-loader": "^0.28.7",
